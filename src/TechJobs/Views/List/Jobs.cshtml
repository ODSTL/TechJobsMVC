<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@{ 
    // TODO #3 - Use the same list display from Views/Search/Index.cshtml 
}

@foreach (var job in ViewBag.Jobs)
{
    <table class="job-listing">
        <tbody>
            @foreach (var item in job)
            {
                <tr>
                    <td>@item.Key</td>
                    <td>@item.Value</td>
                </tr>
            }
        </tbody>
    </table>
}